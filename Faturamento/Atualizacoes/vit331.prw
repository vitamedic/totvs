User Function VIT331Ger(cAlias,nReg,nOpc)


//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³ Declaracao de Variaveis                                             ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
Local nOpcao
Local cTipo
Local cSerie
Local cNota
Local cCliFor
Local cLoja

Local aRetNF

cTipo 	:= "1"

If cTipo == "1"
	cSerie  := SF2->F2_SERIE
	cNota	:= SF2->F2_DOC
	cClieFor:= SF2->F2_CLIENTE
	cLoja	:= SF2->F2_LOJA
	aMotivoCont :=" "
	cVerAmb      := "2.00"
	cAmbiente	  := " "
	cNotaOri  := " "
	cSerieOri := " "
Else
	cSerie  := SF1->F1_SERIE
	cNota	:= SF1->F1_DOC
	cClieFor:= SF1->F1_FORNECE
	cLoja	:= SF1->F1_LOJA
	aMotivoCont 	  :=" "
	cVerAmb     	  := "2.00"
	cAmbiente		  := " "
	cNotaOri  := " "
	cSerieOri := " "

EndIf

aRetNF := U_XmlNfeSef(cTipo,cSerie,cNota,cClieFor,cLoja,cNotaOri,cSerieOri)

nOpcao := Aviso("XML NF-e",aRetNf[2], {"Ok"})

MemoWrite("c:\temp\Chave.txt", aRetNF[1])
Memowrite("c:\temp\NFe.XML", aRetNF[2])

Return